{
	"rewrites": [
		{
			"source": "/install.sh",
			"destination": "https://raw.githubusercontent.com/dogxii/dxcode/main/install.sh"
		}
	],
	"headers": [
		{
			"source": "/install.sh",
			"headers": [
				{
					"key": "Content-Type",
					"value": "text/plain; charset=utf-8"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				}
			]
		}
	]
}
